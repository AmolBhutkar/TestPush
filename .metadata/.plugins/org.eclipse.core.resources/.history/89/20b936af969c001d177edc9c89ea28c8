package Programs;

public class UpperLower {

	public static void main(String[] args) {
		String str="UperCaseLatter";
		int upper=0;
		int lower=0;
		for(int i =0;i<str.length();i++) {
			char ch=str.charAt(i);
			if(ch=='A' && ch=='a') {
				upper++;
			}
		}
		System.out.println("String is upper:"+ upper);
		

	}

}
